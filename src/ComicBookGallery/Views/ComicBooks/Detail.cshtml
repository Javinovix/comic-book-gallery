@{
    Layout = null;

    
 }


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.SeriesTitle</h1>
        <h2>Issue# @ViewBag.Issue</h2>
        <h5>Description:</h5>

            <div>@Html.Raw(ViewBag.Description)</div>
        @if (ViewBag.Artist.Length > 0)
        {
            <Artist>Artist:</Artist>
            <div>
                <ul>
                 @foreach (var art in ViewBag.Artist)
                 {
                        <li>@art</li>
                 }
                </ul>
            </div>
        }
        
    </div>
</body>
</html>
